  <!--================Cart Area =================-->
  <section class="cart_area section_padding">
    <div class="container">
      <div class="cart_inner">
        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Dress</th>

                <th scope="col">Size</th>
                <th scope="col">Price</th>
              </tr>
            </thead>
            <tbody>
              <% if (user.mycart){%>
              <% user.mycart.forEach((cart)=>{%>
              <tr>
                <td>
                  <div class="media">
                    <div class="d-flex">
                      <img class="imgc"src = "<%= cart.link %>">
                    </div>
                    <div class="media-body">
                      <p><%= cart.dressname %></p>
                    </div>
                  </div>
                </td>
                <td>
                  <h5><%= cart.size %></h5>
                </td>

                <td>
                  <h5><%= cart.price %></h5>
                </td>
              </tr>
              <%})%>
            </tbody>
            <%}else{%>
                                          <p>Done</p>
                                          <%}%>
          </table>
          <div class="checkout_btn_inner float-right">
            <a class="btn_1" href="/shopnow">Continue Shopping</a>
            <a class="btn_1 checkout_btn_1" href="checkout">Proceed to checkout</a>
          </div>
        </div>
      </div>
  </section>
  <!--================End Cart Area =================-->
