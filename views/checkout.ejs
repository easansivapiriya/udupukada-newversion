    <!--================login_part Area =================-->
    <section class="login_part section_padding ">
        <div class="container">
            <div class="row align-items-center">
              <div class="col-lg-6 col-md-6">
                  <div class="login_part_form">
                      <div class="login_part_form_iner">
                          <h1> Welcome Back ! <br>
                              Biiling Details</h1> <br>
                          <form class="row contact_form" action="/checkout" method="post" novalidate="novalidate">
                              <div class="col-md-12 form-group p_star">
                                <input
          type="text"
          id="z1"
          name="address"
          class="form-control"
          placeholder="Enter Your Address" required

        />
                              </div>
                              <div class="col-md-12 form-group p_star">
                                <input
        type="text"
        id="z1"
        name="city"
        class="form-control"
        placeholder="Enter Your city" required

      />
                              </div>
                                <div class="col-md-12 form-group p_star">
                              <input
      type="number"
      id="z1"
      name="number"
      class="form-control"
      placeholder="Enter Your number" required

    />
  </div>
                              <div class="col-md-12 form-group">


                                      <input  class="btn_3" onclick="myFunction()" type="submit" name="submit" value="Order now">


                              </div>
                          </form>
                      </div>
                  </div>
              </div>
                <div class="col-lg-6 col-md-6">
                    <div class="login_part_text text-center">
                        <div class="login_part_text_iner">
                            <h2>Your Order </h2>
                            <section class="cart_area section_padding">
                              <div class="container">
                                <div class="cart_inner">
                                  <div class="table-responsive">
                                    <table class="table">
                                      <thead>
                                        <tr>
                                          <th scope="col" style="color:white;">Dress</th>
                                          <th scope="col"></th>

                                          <th scope="col" style="color:white;">Price</th>
                                        </tr>
                                      </thead>
                                      <tbody>
                                        <% if (user.mycart){%>
                                        <% user.mycart.forEach((cart)=>{%>
                                        <tr>
                                          <td>
                                            <div class="media">
                                              <div class="d-flex">
                                                <img class="imgc"src = "<%= cart.link %>">
                                              </div>
                                              <div class="media-body">
                                                <p><%= cart.dressname %></p>
                                              </div>
                                            </div>
                                          </td>
                                          <td></td>
                                                <div class="media-body">

                                          <td>
                                            <h5 style="color:white;"><%= cart.price %></h5>
                                          </td>
                                        </div>
                                        </tr>
                                        <%})%>
                                      </tbody>
                                      <%}else{%>
                                                                    <p>Done</p>
                                                                    <%}%>
                                    </table>

                                  </div>
                                </div>
                            </section>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>
